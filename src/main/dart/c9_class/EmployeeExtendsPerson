class Person {
  String? firstName;
//  Person(Map data){}
//  Person(){}
  Person.fromJson(Map data) {
    print('in Person');
  }
}

class Employee extends Person {
  // Person does not have a default constructor;
  // you must call super.fromJson().
  Employee.fromJsons(super.data) : super.fromJson() {
    print('in Employee');
    print(data);
  }
//  Employee(super.data);
//  Employee(super.data){
